{
    "build": {
        "beforeDevCommand": "",
        "beforeBuildCommand": "",
        "devPath": "../src",
        "distDir": "../src",
        "withGlobalTauri": true
    },
    "package": {
        "productName": "vrsjmp",
        "version": "0.0.1"
    },
    "tauri": {
        "macOSPrivateApi": true,
        "allowlist": {
            "all": false
        },
        "bundle": {
            "active": true,
            "targets": "all",
            "identifier": "com.vrs.vrsjmp",
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ]
        },
        "security": {
            "csp": null
        },
        "windows": [
            {
                "fullscreen": false,
                "resizable": false,
                "alwaysOnTop": true,
                "maximizable": false,
                "minimizable": false,
                "closable": false,
                "transparent": true,
                "title": "vrsjmp",
                "decorations": false,
                "width": 640,
                "height": 461,
                "center": true,
                "acceptFirstMouse": true
            }
        ]
    }
}
